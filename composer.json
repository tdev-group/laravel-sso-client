{
    "name": "tdev-group/laravel-sso-client",
    "description": "Laravel Sso Client provides identity server client support to laravel",
    "keywords": [
        "laravel",
        "oauth",
        "is4",
        "identityserver4",
        "identity-server4"
    ],
    "authors": [
        {
            "name": "Terro",
            "email": "wali7981@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": "^7.1|^8.0",
        "firebase/php-jwt": "^5.5",
        "illuminate/http": "^5.6|^8.0",
        "illuminate/auth": "^5.6|^8.0",
        "illuminate/database": "^5.6|^8.0",
        "illuminate/support": "^5.6|^8.0",
        "illuminate/validation": "^5.6|^8.0",
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelSsoClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSsoClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSsoClient\\Providers\\SsoServiceProvider"
            ]
        }
    }
}